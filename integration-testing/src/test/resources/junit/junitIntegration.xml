<?xml version='1.1' encoding='UTF-8'?>
<!-- @formatter:off -->
<test type="container">
    <test type="case" name="all">
        <bean>launcher</bean>
        <method>execute</method>
        <request type="arrayList">
            <item type="selClass">io.github.dimkich.integration.testing.junit.SimpleTest</item>
        </request>
        <response type="arrayList">
            <response>before root</response>
            <response>before #1</response>
            <response>#1</response>
            <response>after #1</response>
            <response>before #2</response>
            <response>before #2#1</response>
            <response>#2#1</response>
            <response>after #2#1</response>
            <response>before #2#2</response>
            <response>before #2#2#1</response>
            <response>#2#2#1</response>
            <response>after #2#2#1</response>
            <response>before #2#2#2</response>
            <response>#2#2#2</response>
            <response>after #2#2#2</response>
            <response>after #2#2</response>
            <response>before #2#3</response>
            <response>#2#3</response>
            <response>after #2#3</response>
            <response>after #2</response>
            <response>before #3</response>
            <response>#3</response>
            <response>after #3</response>
            <response>before #4</response>
            <response>before #4#1</response>
            <response>#4#1</response>
            <response>after #4#1</response>
            <response>before #4#2</response>
            <response>before #4#2#1</response>
            <response>#4#2#1</response>
            <response>after #4#2#1</response>
            <response>before #4#2#2</response>
            <response>#4#2#2</response>
            <response>after #4#2#2</response>
            <response>after #4#2</response>
            <response>before #4#3</response>
            <response>#4#3</response>
            <response>after #4#3</response>
            <response>after #4</response>
            <response>before #5</response>
            <response>#5</response>
            <response>after #5</response>
            <response>after root</response>
            <response>afterTests root</response>
        </response>
    </test>
    <test type="case" name="first">
        <bean>launcher</bean>
        <method>execute</method>
        <request type="arrayList">
            <item type="selUniqueId">[dynamic-test:#1]</item>
        </request>
        <response type="arrayList">
            <response>before root</response>
            <response>before #1</response>
            <response>#1</response>
            <response>after #1</response>
            <response>after root</response>
            <response>afterTests root</response>
        </response>
    </test>
    <test type="case" name="several">
        <bean>launcher</bean>
        <method>execute</method>
        <request type="arrayList">
            <item type="selUniqueId">[dynamic-test:#1]</item>
            <item type="selUniqueId">[dynamic-container:#2]/[dynamic-container:#2]</item>
            <item type="selUniqueId">[dynamic-container:#4]</item>
        </request>
        <response type="arrayList">
            <response>before root</response>
            <response>before #1</response>
            <response>#1</response>
            <response>after #1</response>
            <response>before #2</response>
            <response>before #2#2</response>
            <response>before #2#2#1</response>
            <response>#2#2#1</response>
            <response>after #2#2#1</response>
