            <response>before #2#2#1</response>
            <response>#2#2#1</response>
            <response>after #2#2#1</response>
            <response>after #2#2</response>
            <response>after #2</response>
            <response>after root</response>
            <response>afterTests root</response>
        </response>
    </test>
    <test type="case" name="filter last">
        <bean>launcher</bean>
        <method>filter</method>
