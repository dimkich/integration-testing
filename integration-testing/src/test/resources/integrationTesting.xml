<?xml version='1.1' encoding='UTF-8'?>
<!-- @formatter:off -->
<test type="container">
    <test type="container" name="mockInvoke">
        <mockInvoke name="converter" method="convertToString">
            <arg type="integer">1</arg>
            <result>
                <return>1</return>
            </result>
            <result>
                <return>2</return>
            </result>
            <result>
                <throw type="httpClientErrorException.BadRequest">
                    <statusCode>BAD_REQUEST</statusCode>
                    <rawStatusCode>400</rawStatusCode>
                    <message>Not supported yet.</message>
                </throw>
            </result>
            <result>
                <return>4</return>
            </result>
        </mockInvoke>
        <test type="case" name="1">
            <bean>converter</bean>
            <method>convertToString</method>
            <request type="integer">1</request>
            <response>1</response>
        </test>
        <test type="case" name="2">
            <bean>converter</bean>
            <method>convertToString</method>
            <request type="integer">1</request>
            <response>2</response>
        </test>
        <test type="case" name="3">
            <bean>converter</bean>
            <method>convertToString</method>
            <request type="integer">1</request>
            <response type="httpClientErrorException.BadRequest">
                <statusCode>BAD_REQUEST</statusCode>
                <rawStatusCode>400</rawStatusCode>
                <message>Not supported yet.</message>
            </response>
        </test>
        <test type="case" name="4">
            <bean>converter</bean>
            <method>convertToString</method>
            <request type="integer">1</request>
            <response>4</response>
        </test>
        <test type="case" name="5">
            <bean>converter</bean>
            <method>convertToString</method>
            <request type="integer">1</request>
            <response>1</response>
        </test>
    </test>
</test>
